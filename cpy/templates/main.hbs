import { AppConfig, createApp } from "@tsdiapi/server";
{{#if plugins}}
{{#each plugins}}
import {{importPackageName}} from "@tsdiapi/{{packageName}}";
{{/each}}
{{/if}}

{{#if plugins}}
createApp({ 
    plugins: [
    {{#each plugins}}
        {{this}}Plugin(),
    {{/each}}
    ]
});
{{else}}
createApp();
{{/if}}